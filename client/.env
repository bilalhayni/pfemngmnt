# Backend API URL
REACT_APP_API_URL=http://localhost:3001

# App Configuration
REACT_APP_NAME="PFE Manager"
REACT_APP_VERSION=1.0.0

# Environment
NODE_ENV=development

# Features
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_TRACKING=false

# Cookie Settings (use true in production with HTTPS)
REACT_APP_COOKIE_SECURE=false
REACT_APP_COOKIE_SAME_SITE=lax

# Session timeout in milliseconds (24 hours)
REACT_APP_SESSION_TIMEOUT=86400000
